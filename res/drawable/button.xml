<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff0d76e1" android:endColor="#ff0d76e1" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#fff403c9" />
            <corners android:radius="6.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ffffc2b7" android:endColor="#ffffc2b7" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#fff403c9" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="0.5dip" android:color="#ffff0000" />
            <corners android:radius="6.0dip" />
        </shape>
    </item>
</selector>